{%extends "mmelectric/templates/temp-sections-mmelectric.html.twig"%}
{% block title%}
	FAQs - M&M Electric of Central Florida, Inc.
{% endblock title%}
{% block stylesheets %}
	<link rel="stylesheet" href="{{asset('/resources/mmelectric/css/custom/main.css')}}"/>
	<link rel="stylesheet" href="{{asset('/resources/mmelectric/css/custom/docs-temp.css')}}"/>
{% endblock %}

{%block quickQuote%}

{%endblock quickQuote%}

{%block content%}
	<div class="container info-cont">
		<h2 class="text-center docs-title">Frequently Asked Questions</h2>
		{%for faqs_article in faqs%}
			<h4 class="item-title">{{faqs_article['title']|raw}}</h4>
			<div class="col-xs-12">				
				{{faqs_article['content']|raw}}
			</div>
		{%endfor%}
	</div>
{%endblock content%}